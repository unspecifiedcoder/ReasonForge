      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "title": "Avg Response Time",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(miner_response_duration_seconds_sum[5m]) / rate(miner_response_duration_seconds_count[5m])",
          "legendFormat": "Avg Latency"
        }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "id": 4,
      "title": "Miner Score Over Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "miner_score",
          "legendFormat": "UID {{uid}}"
        }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
